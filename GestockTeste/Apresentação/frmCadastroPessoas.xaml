<Window x:Class="GestockTeste.Apresentação.CadastroPessoas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GestockTeste.Apresentação"
        mc:Ignorable="d"
        Title="CadastroPessoas" Height="450" Width="800">
    <Grid>
        <Button Content="Cadastrar" HorizontalAlignment="Left" Margin="658,370,0,0" VerticalAlignment="Top" Width="110" Height="37"/>
        <TextBox x:Name="txtNomePessoa" HorizontalAlignment="Left" Margin="252,48,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="148"/>
        <TextBox x:Name="txtCPFPessoa" HorizontalAlignment="Left" Margin="250,102,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="148"/>
        <TextBox x:Name="txtDataDeNascimento" HorizontalAlignment="Left" Margin="252,163,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="148"/>
        <Label x:Name="lblNomePessoa" Content="Nome" HorizontalAlignment="Left" Margin="305,17,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblCPFPessoa" Content="CPF" HorizontalAlignment="Left" Margin="311,71,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblDataDeNascimento" Content="Data de Nascimento" HorizontalAlignment="Left" Margin="267,132,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnVoltar" Content="Voltar" HorizontalAlignment="Left" Margin="54,359,0,0" VerticalAlignment="Top" Width="106" Height="48"/>
        <TextBox x:Name="txtCargo" HorizontalAlignment="Left" Margin="255,222,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="143"/>
        <Label x:Name="lblCargo" Content="Cargo" HorizontalAlignment="Left" Margin="308,191,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtId" HorizontalAlignment="Left" Margin="255,271,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="143"/>
        <Label x:Name="lblId" Content="Id" HorizontalAlignment="Left" Margin="318,245,0,0" VerticalAlignment="Top" Width="21"/>

    </Grid>
</Window>
