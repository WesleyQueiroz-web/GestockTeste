<Window x:Class="GestockTeste.Apresentação.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GestockTeste.Apresentação"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnLogin" Content="Entrar" HorizontalAlignment="Center" Margin="0,292,0,0" VerticalAlignment="Top" Width="128" Height="62"/>
        <TextBox x:Name="txtLogin" HorizontalAlignment="Center" Margin="0,78,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="168" Height="30"/>
        <TextBox x:Name="txtSenha" HorizontalAlignment="Center" Margin="0,186,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="168" Height="31"/>
        <Label x:Name="lblSenhaLogin" Content="Senha" HorizontalAlignment="Center" Margin="0,146,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblUsuário" Content="Usuário" HorizontalAlignment="Center" Margin="0,39,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
